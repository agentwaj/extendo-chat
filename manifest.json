{
  "manifest_version": 3,
  "name": "Extendo Chat",
  "description": "Remove trim from Messenger message preview.",
  "version": "1.3.0",
  "content_scripts": [
    {
      "matches": ["https://www.messenger.com/*"],
      "css": ["messenger.css"]
    },
    {
      "matches": ["https://www.instagram.com/*"],
      "css": ["instagram.css"]
    },
    {
      "matches": ["https://web.whatsapp.com/*"],
      "css": ["whatsapp.css"]
    }
  ]
}
